{
  "type": "array",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "items": {
    "type": "object",
    "required": [
      "alias",
      "city",
      "currency",
      "id",
      "language",
      "title"
    ],
    "properties": {
      "alias": {
        "type": "string"
      },
      "city": {
        "anyOf": [
          {
            "type": "object",
            "required": [
              "id",
              "latitude",
              "longitude",
              "prefix",
              "title"
            ],
            "properties": {
              "id": {
                "type": "integer"
              },
              "latitude": {
                "type": "number"
              },
              "longitude": {
                "type": "number"
              },
              "prefix": {
                "type": "string"
              },
              "title": {
                "type": "string"
              }
            }
          },
          {
            "type": "null"
          }
        ]
      },
      "currency": {
        "type": "object",
        "required": [
          "symbol",
          "symbolFirst",
          "title"
        ],
        "properties": {
          "symbol": {
            "type": "string"
          },
          "symbolFirst": {
            "type": "boolean"
          },
          "title": {
            "type": "string"
          }
        }
      },
      "id": {
        "type": "integer"
      },
      "language": {
        "anyOf": [
          {
            "type": "object",
            "required": [
              "alias"
            ],
            "properties": {
              "alias": {
                "type": "string"
              }
            }
          },
          {
            "type": "null"
          }
        ]
      },
      "title": {
        "type": "string"
      }
    }
  }
}